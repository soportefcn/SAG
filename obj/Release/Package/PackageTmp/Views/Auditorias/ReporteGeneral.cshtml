                {
                    <option value="@Html.Raw((@i + 1).ToString())"@if (@ViewBag.DesdeMes == (i + 1))
                                                                    {@Html.Raw("selected=\"selected\"")}>@Html.Raw(@Meses[@i])</option>
                }
                </select>
                <select name="DesdePeriodo" id="DesdePeriodo">
