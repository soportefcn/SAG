                {
                    <option value="@Html.Raw((@i).ToString())" @if (@ViewBag.DesdePeriodo == i)
                                                                {@Html.Raw("selected=\"selected\"")}>@Html.Raw((@i).ToString())</option>
                }
                </select>
                a
                <select name="HastaMes" id="HastaMes">
