                                                                {@Html.Raw("selected=\"selected\"")}>@Html.Raw((@i).ToString())</option>
                }
                </select>
                <input type="button" value="Aceptar" id="informeGeneral" class="informeGeneral" />
            </td>
        </tr>
        <tr>
            <td>TOTAL DE INFORMES</td>
            <td>@ViewBag.TotalInformes</td>
        </tr>
    </table>
    <br />
    @if (@ViewBag.TotalInformes < 1)
 { 
