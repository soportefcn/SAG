@model SAG2.Models.Dependencia
@{
    ViewBag.Title = "Ingresar registro";
}
<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
<style>
/* Conversion a mayusculas */
input[type="text"], textarea
{
    text-transform: uppercase;
}
</style>
 <script type="text/javascript">
        $(document).ready(function() {
          $(".js-autocomplete").select2();
        });
    </script>
<div id="SAGBox">
    <h3>Dependencia</h3>

    <div id="contenido">
    @ViewBag.Mensaje
@using (Html.BeginForm()) {
    @Html.ValidationSummary(true)
    <fieldset>

     <div class="panel panel-default">
    <div class="panel-heading"><h3>Dato de dependencia</h3></div>
    <div class="panel-body">
      <div class ="form-group form-inline">
            <div class="form-group" style="margin-right: 30px;">
            @Html.LabelFor(model => model.Nombre)
        </div>
        <div class="form-group">
           <input class="form-control" type="text" value="" name="Nombre" id="Nombre" style ="width: 400px">
           
        </div>
     </div>
       <div class ="form-group form-inline">
            <div class="form-group" style="margin-right: 30px;">
           <label>Programa</label> 

        </div>
        <div class="form-group">


        <select name="ProyectoID" id="ProyectoID" class="js-autocomplete">               
                    <option selected value="@ViewBag.ProyectoID">@ViewBag.NombreEstado</option>          
                    <option value="0">Todos</option>
        </select>
           
        </div>
     </div>
  </div>
  <div class="panel-footer">
	<div  style ="width: 500px ; display:inline-block">
             <a class="btn btn-success" href="./Index" onclick="window.open(this.href, 'Buscar','width=700,height=350,scrollbars=yes,menubar=no,toolbars=no'); return false"><span class ="fa fa-search fa-fw"></span>BUSCAR</a>
        <a class="btn btn-default" href="@System.Configuration.ConfigurationManager.AppSettings["prePath"].ToString()/Dependencias/Create"><span class ="fa fa-file-o  fa-fw"></span>NUEVO</a>
        </div>
         <div style="float :right;display:inline-block">
	 
              <input class="btn btn-primary" type="submit" value="Guardar" />
	</div>

  </div>
</div>
         @if (TempData["Message"] != null)
{
    @Html.Raw("<script>alert('"+TempData["Message"]+".');</script>");
}


    </fieldset>
}
    </div>
</div>