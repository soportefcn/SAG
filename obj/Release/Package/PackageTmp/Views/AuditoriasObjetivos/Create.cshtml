@model SAG2.Models.AuditoriasObjetivo

@{
    ViewBag.Title = "Create";
}
<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
<div id="SAGBox">
    <h3>Objetivos de Auditoría</h3>
    <div id="controles">
        
    </div>
    <div id="contenido">
@using (Html.BeginForm()) {
    @Html.ValidationSummary(true)
    <fieldset>
        
  <div class="panel panel-default">
  <div class="panel-heading"><h3><b>Objetivos de Auditoría</b></h3></div>
  <div class="panel-body">
      <div class="form-inline form-group">
       <div class="form-group" style="margin-right: 10px">
            @Html.LabelFor(model => model.Orden)
        </div>
        <div class="form-group">
            @Html.EditorFor(model => model.Orden)
        </div>
    </div>
       <div class="form-inline form-group">
        <div class="form-group" style="margin-right: 10px">
            @Html.LabelFor(model => model.Activo)
        </div>
        <div class="form-group" >
            @Html.EditorFor(model => model.Activo)
        </div></div>
    
