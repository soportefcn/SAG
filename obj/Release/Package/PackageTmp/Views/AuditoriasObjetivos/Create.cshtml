            @Html.LabelFor(model => model.Objetivo)
        </div>
        <div class="form-group">
             @Html.TextAreaFor(model =>model.Objetivo, new {@class="form-control" })
           
        </div>
    
  </div>
  <div class="panel-footer">
	<div  style ="width: 250px ; display:inline-block">
