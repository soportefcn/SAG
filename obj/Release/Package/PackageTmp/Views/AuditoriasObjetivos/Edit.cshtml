@model SAG2.Models.AuditoriasObjetivo

@{
    ViewBag.Title = "Edit";
}
<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
<div id="SAGBox">
    <h3>Objetivos de Auditoría</h3>
    <div id="controles">
       
    </div>
    <div id="contenido">
@using (Html.BeginForm()) {
    @Html.ValidationSummary(true)
    <fieldset>
        

        @Html.HiddenFor(model => model.ID)
        <div class="panel panel-default">
  <div class="panel-heading"><h3><b>Objetivos de Auditoría</b></h3></div>
  <div class="panel-body">

      <div class="form-inline form-group">
       <div class="form-group" style="margin-right: 10px">
            @Html.LabelFor(model => model.Orden)
        </div>
        <div class="form-group">
            @Html.EditorFor(model => model.Orden)
        </div></div>

          <div class="form-inline form-group">
       <div class="form-group" style="margin-right: 10px">
            @Html.LabelFor(model => model.Activo)
        </div>
        <div class="form-group">
            @Html.EditorFor(model => model.Activo)
        </div></div>
      <div class="form-group">
            @Html.LabelFor(model => model.Objetivo)
        </div>
        <div class="form-group">
          
             @Html.TextArea("Objetivo", new { @class="form-control", @value = Model.Objetivo })
        </div>
  </div>
  <div class="panel-footer">
	<div  style ="width: 250px ; display:inline-block">
          <input class="btn btn-primary" type="submit" value="Guardar" />  
        </div>
         <div style="float :right;width:500px;display:inline-block">
	        <a class="btn btn-success" data-toggle="tooltip" data-placement="top" title="Buscar" href="../Index" onclick="window.open(this.href, 'Buscar','width=750,height=400,scrollbars=yes,menubar=no,toolbars=no'); return false"><span class ="fa fa-search fa-lg"></span></a>
            <a class="btn btn-default" data-toggle="tooltip" data-placement="top" title="Nuevo" href="@System.Configuration.ConfigurationManager.AppSettings["prePath"].ToString()/AuditoriasObjetivos/Create"><span class ="fa fa-file-o  fa-lg"></span></a>
            <a class="btn btn-danger" data-toggle="tooltip" data-placement="top" title="Eliminar" href="../Delete/@Html.Raw(Model.ID.ToString())" onclick="return confirm('Realmente desea borrar este registro?');"><span class ="fa fa-trash-o fa-fw"></span></a>
            <a class="btn btn-default" data-toggle="tooltip" data-placement="top" title="Cancelar" href="@System.Configuration.ConfigurationManager.AppSettings["prePath"].ToString()/AuditoriasObjetivos/Create"><span class ="fa fa-undo fa-fw"></span></a>
	</div>

  </div>
</div>
        

       

        <p>
             
        </p>
    </fieldset>
}
    </div>
</div>
