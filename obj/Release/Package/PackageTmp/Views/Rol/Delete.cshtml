@model SAG2.Models.Rol
@{
    ViewBag.Title = "Eliminar";
}
<div id="SAGBox">
    <h3>Roles de usuario</h3>
    <div id="controles">
        <p>@Html.ActionLink("ASIGNAR", "Create")</p>
        <p>@Html.ActionLink("ROLES", "Index")</p>
        <p>@Html.ActionLink("CANCELAR", "Index")</p>
    </div>
    <div id="contenido">
        <fieldset>
            <h3>Realmente desea eliminar este registro?</h3>
        @using (Html.BeginForm())
        {
            <p>
                <input type="submit" value="Guardar" />
            </p>
        }
        </fieldset>
    </div>
</div>