
@{
      
    Layout = null;
    Response.Clear();
    Response.Buffer = true;
    Response.ContentType = "application/vnd.ms-excel";
    Response.Charset = "utf-8";
    Response.AddHeader("Content-Disposition", "attachment;filename=" + ViewBag.SiglaRep + "" + @ViewBag.ID + "_" + DateTime.Now.ToString("yyyyMMdd") + ".xls");


    string FechaSis = DateTime.Now.ToShortDateString();
    int strTipo = int.Parse(ViewBag.TipoReporte);

}
<!DOCTYPE html>
<html>
    <head>
        <title>Reporte Programa Estandar</title>
          <style>
            .text{
                mso-number-format:"\@@"; /*force text*/
                white-space: nowrap;
                text-align: left;
                mso-protection:locked;
            }

            .por100 {
                white-space: nowrap;
                mso-number-format:"Percent";
                text-align: right;
                mso-number-format:"\[Black\]\#\,\#\#0\.00%\;\[Red\]\\-\#\,\#\#0\.00%\;0\.00%";
            }

            .num {
                white-space: nowrap;
                text-align: right;
                mso-number-format:"\[Blue\]\$#\,\#\#0_ \;\[Red\]\-\$#\,\#\#0\;\[Blue\]\$0";
            }
        </style>
    </head>
    
    <body>
    @switch (strTipo)
    {
        case 1:
            @RenderPage("_RealExcel.cshtml"); 
            break;
        case 2:
            @RenderPage("_SintesisReal.cshtml");
            break;
        case 3:
            @RenderPage("_EstandarRealExcel.cshtml");
            break;

    }
    </body>

</html>
