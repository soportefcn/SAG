      catch (Exception)
      { }


      decimal desv_ing_1sem = 0;
      decimal desv_ing_2sem = 0;
      
      try
      {
          desv_ing_1sem = 100 * (prod_aportesubvencion_ing_1 + prod_aportesubvencion_ing_2 - ViewBag.Presupuesto_Ingresos_1) / ViewBag.Presupuesto_Ingresos_1;
      }
      catch(Exception)
      {
          desv_ing_1sem = 100 * (prod_aportesubvencion_ing_1 + prod_aportesubvencion_ing_2 - ViewBag.Presupuesto_Ingresos_1);
      }

      try
      {
          desv_ing_1sem = 100 * (prod_aportesubvencion_ing_3 + prod_aportesubvencion_ing_4 - ViewBag.Presupuesto_Ingresos_2) / ViewBag.Presupuesto_Ingresos_2;
      }
      catch (Exception)
