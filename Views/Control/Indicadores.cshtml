  <tr title="Se obtiene de los antecedentes del proyecto (Nº Plazas Convenio)">
    @{
        int indicador_cobertura = @ViewBag.Indicadores.CapCobertura;
        int cobertura_1 = @ViewBag.Cobertura_1;
        int cobertura_2 = @ViewBag.Cobertura_2;
        int cobertura_3 = @ViewBag.Cobertura_3;
        int cobertura_4 = @ViewBag.Cobertura_4;

        int cobertura_1sem = cobertura_1 + cobertura_2;
        int cobertura_2sem = cobertura_3 + cobertura_4;

        int cobertura_año = cobertura_1 + cobertura_2 + cobertura_3 + cobertura_4;
    }
    <th rowspan="2" align="left" title="">Capacidad</th>
    <th align="left">Cobertura</th>
    <td align="center">Nº Niños/Mes</td>
    <td align="center">@indicador_cobertura.ToString()</td>
    <td align="center">Trimestral</td>
    <td align="center"@if (cobertura_1 / 3 >= indicador_cobertura)
                     {@Html.Raw(" style=\"color: blue;\"")}
                     else
                     {@Html.Raw(" style=\"color: red;\"")}>@((cobertura_1 / 3).ToString("#,##0"))</td>
    <td align="center"@if (cobertura_2 / 3 >= indicador_cobertura)
                     {@Html.Raw(" style=\"color: blue;\"")}
                     else
                     {@Html.Raw(" style=\"color: red;\"")}>@((cobertura_2 / 3).ToString("#,##0"))</td>
    <td align="center"@if (cobertura_1sem / 6 >= indicador_cobertura)
                     {@Html.Raw(" style=\"color: blue;\"")}
                     else
                     {@Html.Raw(" style=\"color: red;\"")}>@((cobertura_1sem / 6).ToString("#,##0"))</td>
    <td align="center"@if (cobertura_3 / 3 >= indicador_cobertura)
                     {@Html.Raw(" style=\"color: blue;\"")}
                     else
                     {@Html.Raw(" style=\"color: red;\"")}>@((cobertura_3 / 3).ToString("#,##0"))</td>
    <td align="center"@if (cobertura_4 / 3 >= indicador_cobertura)
                     {@Html.Raw(" style=\"color: blue;\"")}
                     else
                     {@Html.Raw(" style=\"color: red;\"")}>@((cobertura_4 / 3).ToString("#,##0"))</td>
    <td align="center"@if (cobertura_2sem / 6 >= indicador_cobertura)
                     {@Html.Raw(" style=\"color: blue;\"")}
                     else
                     {@Html.Raw(" style=\"color: red;\"")}>@((cobertura_2sem / 6).ToString("#,##0"))</td>
    <td align="center"@if (cobertura_año / 12 >= indicador_cobertura)
                     {@Html.Raw(" style=\"color: blue;\"")}
                     else
                     {@Html.Raw(" style=\"color: red;\"")}>@((cobertura_año / 12).ToString("#,##0"))</td>
  </tr>
  <tr title="Se Obtiene del SAG: Intervenciones realizadas Trim. / Cobretura X 3">
    @{
        decimal indicador_intervenciones = 0;
        try
        {
            indicador_intervenciones = @ViewBag.Indicadores.CapIntervenciones;
        }
        catch (Exception)
        {}
        
        int intervenciones_1 = ViewBag.Intervenciones_1;
        int intervenciones_2 = ViewBag.Intervenciones_2;
        int intervenciones_3 = ViewBag.Intervenciones_3;
        int intervenciones_4 = ViewBag.Intervenciones_4;
        int intervenciones_1sem = intervenciones_1 + intervenciones_2;
        int intervenciones_2sem = intervenciones_3 + intervenciones_4;
        int intervenciones_año = intervenciones_1sem + intervenciones_2sem;

        decimal porcentaje_1 = 100;
        decimal porcentaje_2 = 100;
        decimal porcentaje_3 = 100;
        decimal porcentaje_4 = 100;
        decimal procentaje_1sem = 100;
        decimal procentaje_2sem = 100;
        decimal porcentaje_año = 100;

        if (cobertura_1 > 0)
        {
            porcentaje_1 = 100 * intervenciones_1 / cobertura_1;
        }

        if (cobertura_2 > 0)
        {
            porcentaje_2 = 100 * intervenciones_2 / cobertura_2;
        }

        if (cobertura_3 > 0)
        {
            porcentaje_3 = 100 * intervenciones_3 / cobertura_3;
        }

        if (cobertura_4 > 0)
        {
            porcentaje_4 = 100 * intervenciones_4 / cobertura_4;
        }

        if (cobertura_1sem > 0)
        {
            procentaje_1sem = 100 * intervenciones_1sem / cobertura_1sem;
        }

        if (cobertura_2sem > 0)
        {
            procentaje_2sem = 100 * intervenciones_2sem / cobertura_2sem;
        }

        if (cobertura_año > 0)
        {
            porcentaje_año = 100 * intervenciones_año / cobertura_año;
        }
        }
    <th align="left">Intervenciones/Mes</th>
    <td align="center">% Niños/Mes</td>
    <td align="center">@indicador_intervenciones.ToString("#,##0.0#")%</td>
    <td align="center">Trimestral</td>
    <td align="center"@if (porcentaje_1 >= indicador_intervenciones)
                     {@Html.Raw(" style=\"color: blue;\"")}
                     else
                     {@Html.Raw(" style=\"color: red;\"")}>@porcentaje_1.ToString("#,##0.0#")%</td>
    <td align="center"@if (porcentaje_2 >= indicador_intervenciones)
                     {@Html.Raw(" style=\"color: blue;\"")}
                     else
                     {@Html.Raw(" style=\"color: red;\"")}>@porcentaje_2.ToString("#,##0.0#")%</td>
    <td align="center"@if (procentaje_1sem >= indicador_intervenciones)
                     {@Html.Raw(" style=\"color: blue;\"")}
                     else
                     {@Html.Raw(" style=\"color: red;\"")}>@procentaje_1sem.ToString("#,##0.0#")%</td>
    <td align="center"@if (porcentaje_3 >= indicador_intervenciones)
                     {@Html.Raw(" style=\"color: blue;\"")}
                     else
                     {@Html.Raw(" style=\"color: red;\"")}>@porcentaje_3.ToString("#,##0.0#")%</td>
    <td align="center"@if (porcentaje_4 >= indicador_intervenciones)
                     {@Html.Raw(" style=\"color: blue;\"")}
                     else
                     {@Html.Raw(" style=\"color: red;\"")}>@porcentaje_4.ToString("#,##0.0#")%</td>
    <td align="center"@if (procentaje_2sem >= indicador_intervenciones)
                     {@Html.Raw(" style=\"color: blue;\"")}
                     else
                     {@Html.Raw(" style=\"color: red;\"")}>@procentaje_2sem.ToString("#,##0.0#")%</td>
    <td align="center"@if (porcentaje_año >= indicador_intervenciones)
                     {@Html.Raw(" style=\"color: blue;\"")}
                     else
                     {@Html.Raw(" style=\"color: red;\"")}>@porcentaje_año.ToString("#,##0.0#")%</td>
  </tr>
