@model IEnumerable<SAG2.Models.BienProcedencia>
@{
    Layout = "~/Views/Shared/PopUp.cshtml";
    ViewBag.Title = "Lista Procedencia de Bienes";
    SAG2.Classes.Util utils = new SAG2.Classes.Util();
}

<div id="contenido_popup">
    <table class="table table-bordered table-hover">
        <thead class="fixedHeader">
            <tr class="info">
                <th style="width: 33px;" class="controles"></th>
                <th style="width: 49px;">Cód </th>
                <th style="width: 650px;">Nombre </th>
            </tr>
        </thead>
        <tbody class="scrollContent">

            @foreach (var item in Model)
            {
                var style = "";

                <tr @Html.Raw(style)>
                    <td style="width: 33px;">
                        <a href="@Url.Action("Edit", "BienProcedencia", new { id = item.ID })" title="Seleccionar" onclick="window.opener.document.location=this.href; window.close();"><img src="@Url.Content("~/Content/Images/Badge-tick.png")" alt="Seleccionar" width="15" height="15" style="border: 0;" /></a>
                    </td>
                    <td style="width: 49px;" align="center">
                        @Html.DisplayFor(modelItem => item.ID)
                    </td>
                    <td style="width: 650px;">
                        @item.Nombre
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>