@model IEnumerable<SAG2.Models.PorcentajeZona>
@{
    Layout = "~/Views/Shared/PopUp.cshtml";
    ViewBag.Title = "Mantenedor Porcentaje Zona";
    SAG2.Classes.Util utils = new SAG2.Classes.Util();
}

<div id="contenido_popup">
    <table class="table table-bordered table-hover">
        <thead class="fixedHeader">
            <tr class="info">
                <th style="width: 33px;" class="controles"></th>
                <th style="width: 49px;">Cód </th>
                <th style="width: 350px;">Nombre </th>
                <th style="width: 300px;">Valor </th>
                <th style="width: 33px;">Eliminar </th>
            </tr>
        </thead>
        <tbody class="scrollContent">

            @foreach (var item in Model.Where(e => e.estado != 1))
            {
                var style = "";

            <tr @Html.Raw(style)>
                <td style="width: 33px;">
                    <a href="@Url.Action("Edit", "PorcentajeZona", new { id = item.ID })" title="Seleccionar" onclick="window.opener.document.location=this.href; window.close();"><img src="@Url.Content("~/Content/Images/Badge-tick.png")" alt="Seleccionar" width="15" height="15" style="border: 0;" /></a>
                </td>
                <td style="width: 49px;" align="center">
                    @Html.DisplayFor(modelItem => item.ID)
                </td>
                <td style="width: 350px;">
                    @item.Nombre
                </td>
                <td style="width: 300px;">
                    @item.Valor.ToString()
                </td>
                <td style="width: 63px;">
                    <a href="@Url.Action("Delete", "PorcentajeZona", new { id = item.ID })" title="Eliminar" onclick="window.opener.document.location=this.href; window.close();"><img src="@Url.Content("~/Content/Images/Delete.png")" alt="Seleccionar" width="15" height="15" style="border: 0;" /></a>
                </td>
            </tr>
            }
        </tbody>
    </table>
</div>

