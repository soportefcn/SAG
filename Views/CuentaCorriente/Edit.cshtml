@model SAG2.Models.CuentaCorriente
@{
    ViewBag.Title = "Modificar";
}
<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
<style>
/* Conversion a mayusculas */
input[type="text"], textarea
{
    text-transform: uppercase;
}
</style>
<div id="SAGBox">
    <h3>Cuentas corrientes</h3>
  
    <div id="contenido" class="cuentacorriente">
@using (Html.BeginForm())
{
    @Html.ValidationSummary(true)
    @Html.Hidden("DireccionID", Model.DireccionID)
    <fieldset>

   <div class="panel panel-default">
  <div class="panel-heading"><h3><b>Dato de Cuenta Corriente</b></h3></div>
  <div class="panel-body">
      <ul class="nav nav-tabs" role="tablist" id="myTab">
  <li role="presentation" class="active"><a href="#home" role="tab" data-toggle="tab">Cuenta Corriente</a></li>
  <li role="presentation"><a href="#profile" role="tab" data-toggle="tab">Dato Contacto</a></li>
  
</ul>

<div class="tab-content">
  <div role="tabpanel" class="tab-pane active" id="home">
      <br />
        <div class ="form-group form-inline">
       <div class="form-group" style="margin-right: 58px;margin-left: 7px;">
            @Html.LabelFor(model => model.Numero)
         </div>
        <div class="form-group" style="margin-right: 90px;">
             @Html.TextBoxFor(model => model.Numero, new { @class ="form-control", @tabindex = "1" })
          </div>
              <div class="form-group" style="margin-right: 10px;margin-left: 7px;">
            @Html.LabelFor(model => model.ProyectoID, "Proyecto")
        </div>
        <div class="form-group">
            @Html.DropDownList("EstablecimientoID", null, new { @tabindex = "5", @disabled = "disabled", @class="form-control" })
        </div>
       </div>
      <div class ="form-group form-inline">
       <div class="form-group" style="margin-right: 65px;margin-left: 7px;">
             @Html.LabelFor(model => model.BancoID, "Banco")
        </div>
        <div class="form-group">
            @Html.DropDownList("BancoID", null, new { @tabindex = "2", @class="form-control" })
        </div></div>

      <div class ="form-group form-inline">
       <div class="form-group" style="margin-right: 50px;margin-left: 7px;">
            @Html.LabelFor(model => model.Sucursal)
        </div>
        <div class="form-group">
            @Html.TextBoxFor(model => model.Sucursal, new { @tabindex = "3", @class="form-control" })
        </div></div>

         <div class ="form-group form-inline">
       <div class="form-group" style="margin-right: 10px;margin-left: 7px;">
            @Html.LabelFor(model => model.LineaCredito, "Línea de crédito")
        </div>
        <div class="form-group">
            @Html.TextBoxFor(model => model.LineaCredito, new { @tabindex = "4", @class="form-control" })
        </div></div>

       <div class ="form-group form-inline">
     </div>
      
        <div class ="form-group form-inline">
       <div class="form-group" style="margin-right: 30px;margin-left: 7px;">
            @Html.Label("Dirección")
        </div>
        <div class="form-group">
            @Html.TextBox("N", null, new { @class="form-control", @style = "width: 0; border: 0 solid White;", @disabled = "disabled" }) 
        </div>
            </div>
       <div class ="form-group form-inline">
       <div class="form-group" style="margin-right: 10px;margin-left: 7px;">
            @Html.Label("Calle")
        </div>
        <div class="form-group" style="margin-right: 10px;">
            @Html.TextBoxFor(model => model.Direccion.Calle, new { @class="form-control", @tabindex = "6" }) 
        </div>
        <div class="form-group">
            @Html.Label("Número")
        </div>
        <div class="form-group" style="margin-right: 5px;">
            @Html.TextBoxFor(model => model.Direccion.Numero, new { @class="form-control", @tabindex = "7" }) 
        </div>
        <div class="form-group">
            @Html.Label("Departamento")
        </div>
        <div class="form-group">
            @Html.TextBoxFor(model => model.Direccion.Depto, new { @class="form-control", @tabindex = "8" }) 
        </div></div>

       <div class ="form-group form-inline">
       <div class="form-group" style="margin-right: 30px;margin-left: 7px;">
            @Html.Label("Región")
        </div>
        <div class="form-group">
            @Html.DropDownList("RegionID", null, new { @class="form-control", @tabindex = "9", @comuna = Model.Direccion.ComunaID.ToString() })
        </div>
        <div class="form-group">
            @Html.Label("Comuna")
        </div>
        <div class="form-group">
            <select name="ComunaID" id="ComunaID" tabindex="10" class="form-control">
                <option>Seleccione región</option>
            </select>
        </div>
</div>
  </div>
  <div role="tabpanel" class="tab-pane" id="profile">
      <br />
      <div class ="form-group form-inline">
       <div class="form-group" style="margin-right: 30px;margin-left: 7px;">
            @Html.LabelFor(model => model.Ejecutivo)
        </div>
        <div class="form-group">
            @Html.TextBoxFor(model => model.Ejecutivo, new { @class="form-control", @tabindex = "10", @style="width: 350px;" })
        </div></div>

        <div class ="form-group form-inline">
       <div class="form-group" style="margin-right: 30px;margin-left: 7px;">
            @Html.LabelFor(model => model.Fono)
        <br />
            @Html.TextBoxFor(model => model.Fono, new { @class="form-control", @tabindex = "11" })
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.Fax)
        <br />          
            @Html.TextBoxFor(model => model.Fax, new { @class="form-control", @tabindex = "12" })
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.Movil)
        <br />
            @Html.TextBoxFor(model => model.Movil, new { @class="form-control", @tabindex = "13" })
        </div>
            </div>
       <div class ="form-group form-inline">
       <div class="form-group" style="margin-right: 56px;margin-left: 7px;">
            @Html.LabelFor(model => model.CorreoElectronico, "Email")
        </div>
        <div class="form-group">
            @Html.TextBoxFor(model => model.CorreoElectronico, new { @class="form-control", @tabindex = "14" })
        </div></div>

         <div class ="form-group form-inline">
       <div class="form-group" style="margin-right: 20px;margin-left: 7px;">
            @Html.LabelFor(model => model.PaginaWeb)
        </div>
        <div class="form-group">
            @Html.TextBoxFor(model => model.PaginaWeb, new { @class="form-control", @tabindex = "15" })
        </div></div>

        <div class="form-group">
            @Html.LabelFor(model => model.Comentarios)
        </div>
        <div class="form-group">
            @Html.TextAreaFor(model => model.Comentarios, new { @class="form-control", @tabindex = "16" })
        </div>       

  </div>
  
</div>
  </div>
  <div class="panel-footer">
	<div  style ="width: 500px ; display:inline-block">
            <a class="btn btn-success" href="../Index" onclick="window.open(this.href, 'Buscar','width=530,height=350,scrollbars=yes,menubar=no,toolbars=no'); return false"><span class ="fa fa-search fa-fw"></span>BUSCAR</a>
       <a class="btn btn-default" href="/SAG_5_desarrollo/CuentaCorriente/Create"><span class ="fa fa-file-o  fa-fw"></span>NUEVO</a>
        <a class="btn btn-danger" href="../Delete/@Html.Raw(Model.ID.ToString())" onclick="return confirm('Realmente desea borrar este registro?');"><span class ="fa fa-trash-o fa-fw"></span>ELIMINAR</a>
       <a class="btn btn-default" href="/SAG_5_desarrollo/CuentaCorriente/Create"><span class ="fa fa-undo fa-fw"></span>CANCELAR</a>
        </div>
         <div style="float :right;display:inline-block">
	  
	  <input class="btn btn-primary" type="submit" value="Guardar" tabindex="17" />
         </div>

  </div></div>

   
        
        <p>
           
        </p>
       
        </fieldset>
}
    </div>
</div>