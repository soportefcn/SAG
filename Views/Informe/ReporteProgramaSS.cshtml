        </div>
    </div>
    <div class="form-group">
        @Html.Label("Año", htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-10">
            @Html.DropDownList("periodo", new List<SelectListItem> { new SelectListItem { Value = "2017", Text = "2017" }, new SelectListItem { Value = "2018", Text = "2018" }, new SelectListItem { Value = "2019", Text = "2019" }, new SelectListItem { Value = "2020", Text = "2020" }, new SelectListItem { Value = "2021", Text = "2021" }, new SelectListItem { Value = "2022", Text = "2022" } }, new { @class = "form-control" })
        </div>
