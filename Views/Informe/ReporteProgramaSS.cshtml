@using (Html.BeginForm())
{
    <h2>Parametros de búsqueda</h2>
    <hr />
    @Html.ValidationSummary(false, "", new { @class = "text-danger" })

    <div class="form-group">
        @Html.Label("Nombre Reporte", htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-10">
            @Html.DropDownList("Informe", null, htmlAttributes: new { @class = "form-control" })
        </div>
    </div>
    <div class="form-group">
        @Html.Label("Proyectos", htmlAttributes: new { @class = "control-label col-md-2" })
