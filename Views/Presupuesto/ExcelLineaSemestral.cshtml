                columna_actual++;
                <td class="num">+@celdas[columna_actual-1]5-@celdas[columna_actual-2]5</td>
                columna_actual++;
                <td class="por100">=SI(Y(@celdas[columna_actual-1]5=0;@celdas[columna_actual-3]5=0);0;SI(ESERROR(@celdas[columna_actual-1]5/@celdas[columna_actual-3]5);1;@celdas[columna_actual-1]5/@celdas[columna_actual-3]5))</td>
                columna_actual++;
            }
            }
            <td class="num" style="mso-protection:locked; background-color: silver;">=B5</td>
            <td class="num" style="mso-protection:locked; background-color: silver;">=C5</td>
            <td class="num" style="mso-protection:locked; background-color: silver;">+AA5-Z5</td>
            <td class="por100" style="mso-protection:locked; background-color: silver;">+AB5/Z5</td>
        </tr>
        <!-- Fila 6 -->
        <tr>
            <th>Ingresos</th>
        </tr>
