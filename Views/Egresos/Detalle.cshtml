@model SAG2.Models.DetalleEgreso
@{
    ViewBag.Title = "Egresos";
    SAG2.Models.Cuenta Cuenta = (SAG2.Models.Cuenta)@ViewBag.Cuenta;
    List<SAG2.Models.DetalleEgreso> ListaDetalle = (List<SAG2.Models.DetalleEgreso>)@ViewBag.ListadoDetalle;
}
<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
<div id="SAGBox">
    <h3>@ViewBag.Title</h3>
    <div id="controles">
        <p><a href="./Index" onclick="window.open(this.href, 'Buscar','width=1100,height=400,scrollbars=yes,menubar=no,toolbars=no'); return false">BUSCAR</a></p>
        <p>@Html.ActionLink("NUEVO", "Create")</p>
        <p>@Html.ActionLink("CANCELAR", "Create")</p>
    </div>
    <div id="contenido" class="ingresos">
        <style>
/* Conversion a mayusculas */
input[type="text"], textarea
{
    text-transform: uppercase;
}
</style>
@using (Html.BeginForm())
{
    @Html.Raw(@ViewBag.Mensaje)
    @Html.ValidationSummary(true)

        <fieldset>
            <legend>Detalle comprobante de egreso</legend>
            <div class="form-group">
                @Html.Label("Origen")
            </div>
            <div class="form-group">
                <select name="Origen" id="Origen">
                    <option>Deuda Pendiente</option>
                    <option>Honorarios y Servicios</option>
                    <option>Fondo Fijo</option>
                    <option selected="selected">Otros</option>
                </select>
            </div>

            <div class="form-group">
                @Html.LabelFor(model => model.DocumentoID)
            </div>
            <div class="form-group">
                @Html.DropDownList("DocumentoID", String.Empty)
            </div>

            <div class="form-group">
                @Html.LabelFor(model => model.CuentaID)
            </div>
            <div class="form-group">
                <select name="CuentaID" id="CuentaID">
                    @Html.Raw(@ViewBag.Arbol)
                </select>
            </div>

            <div class="form-group">
                @Html.LabelFor(model => model.Glosa)
            </div>
            <div class="form-group">
                @Html.TextArea("Glosa", new { @cols = "60", @rows = "7" })
            </div>

            <div class="detalle-egreso">
            @foreach (SAG2.Models.DetalleEgreso Detalle in ListaDetalle)
            {
                    
            }
            <div>
        </fieldset>
}
    </div>
</div>