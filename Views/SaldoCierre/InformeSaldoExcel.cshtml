@model IEnumerable<SAG2.Models.SaldosCorporativos>
@{
    ViewBag.Title = "Informe Saldo";
    var Meses = new string[12]
    {
	    "Enero",
	    "Febrero",
	    "Marzo",
	    "Abril",
	    "Mayo",
	    "Junio",
	    "Julio",
	    "Agosto",
	    "Septiembre",
	    "Octubre",
	    "Noviembre",
	    "Diciembre"
	};
    Layout = null;
    Response.Clear();
    Response.Buffer = true;
    Response.ContentType = "application/vnd.ms-excel";
    Response.Charset = "utf-8";
    Response.AddHeader("Content-Disposition", "attachment;filename=SaldoCorp" + @ViewBag.DesdeMes + @ViewBag.DesdePeriodo + DateTime.Now.ToShortDateString() + ".xls");
}

    <table class="table table-bordered table-hover">
    <thead>
        <tr class="info">
            <th>Cuenta</th>
             <th>Saldo Contable</th>
            <th>Retencion</th>
            <th>Retencion</th>
            <th>Saldo Contable</th>
            <th>Saldo Disponible</th>
         </tr>
        </thead>
        <tbody>

@foreach (var item in Model) {
  
      <tr >
        <td>@item.Numerocuenta </td> 
          <td>@item.SaldoContable  </td>           
          <td>@item.Retencion  </td>     
          <td>@item.Retencion1 </td>       
          <td>@item.Saldo_Contable  </td>      
          <td>@item.Saldo_Disponible </td>      
      </tr>

  }
   

    </table>
    


